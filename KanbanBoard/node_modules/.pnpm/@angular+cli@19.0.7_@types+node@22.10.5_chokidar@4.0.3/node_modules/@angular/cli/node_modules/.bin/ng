#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules/@angular/cli/bin/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules/@angular/cli/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules/@angular/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules/@angular/cli/bin/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules/@angular/cli/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules/@angular/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/@angular+cli@19.0.7_@types+node@22.10.5_chokidar@4.0.3/node_modules:/home/johnfieweger/KanbanBoard/KanbanBoard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ng.js" "$@"
else
  exec node  "$basedir/../../bin/ng.js" "$@"
fi
